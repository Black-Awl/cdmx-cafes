---
const navItems = [
  { name: "Home", url: "/" },
  { name: "Cafes", url: "/cafes" },
  { name: "About", url: "/about" },
];
---

<nav class="bg-base-200">
  <div class="container mx-auto px-4">
    <ul class="menu menu-horizontal">
      {navItems.map((item) => (
        <li><a href={item.url}>{item.name}</a></li>
      ))}
    </ul>
  </div>
</nav>
